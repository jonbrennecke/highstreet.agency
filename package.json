{
  "name": "mathhacksco-blog",
  "version": "0.0.8",
  "description": "Mathhacks.co blog",
  "main": "lib/index.js",
  "scripts": {
    "render:production": "cross-env NODE_ENV=production node ./public/server.js",
    "render:staging": "cross-env NODE_ENV=staging node ./public/server.js",
    "render": "node ./public/server.js",
    "build:production": "npm run clean && cross-env NODE_ENV=production webpack && npm run render:production",
    "build:staging": "npm run clean && cross-env NODE_ENV=staging webpack && npm run render:staging",
    "build": "npm run clean && webpack",
    "serve:production": "cd public && python -m SimpleHTTPServer",
    "serve": "node dev-server.js",
    "test": "karma start --single-run --browsers PhantomJS",
    "check": "./scripts/check.sh",
    "clean": "rm -rf public",
    "develop": "npm run build && npm run serve",
    "flow": "flow",
    "eslint": "eslint './src/**/*.js' index.js",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.js\"",
    "gulp": "gulp",
    "generate-favicons": "gulp generate-favicons"
  },
